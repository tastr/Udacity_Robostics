<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".8 .4 .2"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".8 .4 .2"/>
      </geometry>
    </visual>


    <collision name='back_caster_collision'>
      <origin xyz="-0.30 0 -0.1" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
    </collision>

    <visual name='back_caster_visual'>
      <origin xyz="-0.30 0 -0.1" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
    </visual>

    <collision name='front_caster_collision'>
      <origin xyz="0.30 0 -0.1" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
    </collision>

    <visual name='front_caster_visual'>
      <origin xyz="0.30 0 -0.1" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.1"/>
      </geometry>
    </visual>

  </link>

 <link name='front_right_wheel'>


    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='front_right_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
	 <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </collision>

    <visual name='front_right_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
	 <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </visual>

  </link>

 <link name='front_left_wheel'>


    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='front_left_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
	 <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </collision>

    <visual name='front_left_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
	 <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </visual>

  </link>

 <joint type="continuous" name="front_left_wheel_hinge">
    <origin xyz="0.4 0.3 0" rpy="0 0 0"/>
    <child link="front_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
    <commandTopic>cmd_vel</commandTopic>
  </joint>

  <joint type="continuous" name="front_right_wheel_hinge">
    <origin xyz="0.4 -0.3 0" rpy="0 0 0"/>
    <child link="front_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
    <commandTopic>cmd_vel</commandTopic>
  </joint>



 <link name='back_right_wheel'>


    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='back_right_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
	 <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </collision>

    <visual name='back_right_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
	 <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </visual>

  </link>

 <link name='back_left_wheel'>


    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='back_left_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
	 <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </collision>

    <visual name='back_left_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
	 <cylinder length="0.05" radius="0.2"/>
      </geometry>
    </visual>

  </link>

 <joint type="continuous" name="back_left_wheel_hinge">
    <origin xyz="-0.4 0.3 0" rpy="0 0 0"/>
    <child link="back_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
    <commandTopic>cmd_vel</commandTopic>
  </joint>

  <joint type="continuous" name="back_right_wheel_hinge">
    <origin xyz="-0.4 -0.3 0" rpy="0 0 0"/>
    <child link="back_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
    <commandTopic>cmd_vel</commandTopic>
  </joint>


<link name='cabin'>

    <inertial>
      <mass value="7.5"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.05" ixy="0" ixz="0"
          iyy="0.05" iyz="0"
          izz="0.05"
      />
    </inertial>

    <collision name='cabin_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".3 .4 .2"/>
      </geometry>
    </collision>y

    <visual name='cabin_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0 "/>
      <geometry>
        <box size=".3 .4 .2"/>
      </geometry>
    </visual>

  </link>

  <joint name="cabin_joint" type="fixed">
    <origin xyz="0 0 0.2" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="cabin" />
  </joint>


 <link name='camera'>
     <origin xyz="0.0 0 0" rpy=" 0 0 0"/>

    <inertial>
      <mass value=".1"/>
      <origin xyz="0.05 0.05 0.05" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

     <collision name='camera_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".05 .1 .1"/>
      </geometry>
    </collision>

    <visual name='camera_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".05 .1 .1"/>
      </geometry>
    </visual>
  <imageTopicName>rgb/image_raw</imageTopicName>
  </link>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.15 0 0" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="cabin"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>


 <link name='hokuyo'>
     <origin xyz="0.0 0 0" rpy=" 0 0 0"/>

    <inertial>
      <mass value="1e-5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

     <collision name='hokuyo_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".2 .2 .2"/>
      </geometry>
    </collision>

    <visual name='hokuyo_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
	 <mesh filename = "package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>
   <topicName>/scan</topicName>

  </link>

  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="cabin"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>


<gazebo reference="front_left_wheel">
    <material>Gazebo/Black</material>
</gazebo>

<gazebo reference="front_right_wheel">
    <material>Gazebo/Black</material>
</gazebo>

<gazebo reference="back_left_wheel">
    <material>Gazebo/Black</material>
</gazebo>

<gazebo reference="back_right_wheel">
    <material>Gazebo/Black</material>
</gazebo>


<gazebo reference="chassis">
    <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="cabin">
    <material>Gazebo/Yellow</material>
</gazebo>

<gazebo reference="camera">
    <material>Gazebo/Blue</material>
</gazebo>

<gazebo reference="hokuyo">
    <material>Gazebo/Red</material>
</gazebo>
</robot>
